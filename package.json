  "repository": {
    "type": "git",
    "url": "git+https://github.com/GianlucaIavicoli/nuxt3-lucia-auth-template.git"
  },
  "bugs": {
    "url": "https://github.com/GianlucaIavicoli/nuxt3-lucia-auth-template/issues"
  },
  "scripts": {
    "build": "prisma generate && nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.1",
